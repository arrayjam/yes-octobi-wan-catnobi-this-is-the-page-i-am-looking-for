{
  "name": "Yes Octobi Wan Catnobi this is the page I am looking for",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Add a create repo button on github 404 pages.",
  "homepage_url": "http://github.com/arrayjam",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "web_accessible_resources": [
    "images/stormtroopocat.png"
  ],
  "permissions": [
    "webRequest",
    "*://github.com/*/*"
  ],
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*"
      ],
      "js": [
        "src/inject/repo_check.js"
      ]
    },
    {
      "matches": [
        "https://github.com/new"
      ],
      "js": [
        "src/inject/new.js"
      ]
    }
  ]
}